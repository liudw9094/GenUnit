#ifndef CTRJUDGE_H_
#define CTRJUDGE_H_

#include <string>
#include <vector>

class CTRJudge {
private:
	const std::vector<std::string>& m_args;
public:
	CTRJudge(const std::vector<std::string>& args);
	~CTRJudge();

	int run();
private:
	bool parseCommands();
	void showCommands();
};

#endif /* CTRJUDGE_H_ */
